<!DOCTYPE html>
<html><head>
    <meta charset="utf-8">
    <title><%=title %></title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8,Chrome=1">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="format-detection" content="telephone=no">
    <meta name="format-detection" content="email=no">
    <meta http-equiv="Expires" content="-1">
    <meta http-equiv="Cache-Control" content="no-cache">
    <meta http-equiv="Pragma" content="no-cache">
    <!-- uc强制竖屏 -->
    <!-- <meta name="x5-orientation" content="portrait"> -->
    <!-- QQ强制竖屏 -->
    <!-- <meta name="screen-orientation" content="portrait"> -->
    <!-- windows phone 点击无高光 -->
    <!-- <meta name="msapplication-tap-highlight" content="no"> -->

    <script type="text/javascript" src="../static/js/jquery/3.1.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="../static/css/common.css">
    
    <script type="text/javascript">
        $(function(){
            $('.outBox').height($(window).height());
            $(window).resize(function(){
                $('.outBox').height($(window).height());
            });
            window.addEventListener("offline", function(e) {
                alert('当前网络异常，请检查网络')
            });
            window.addEventListener("online", function(e) {});
        });
    </script>
</head>
<body>
<div class="outBox" style="height: 568px;">
    <div class="header bg_white">
        <div class="header_btn pre_btn">
            <i class="layout_icon layout_icon03_01" style="display:none;"></i>
        </div>
        <div class="header_tit">
            <p><%=title %></p>
        </div>
        <div class="header_btn">
            <%if(navType==='login'){%>
                <p class="topRightBtn" onclick="window.location.href='/signup'">注册</p>
            <%}else if(navType==='signup'){%>
                <p class="topRightBtn" onclick="window.location.href='/login'" >登陆</p>
            <%}else if(navType==='worklist'){%>
                <div class="helpBtn btn_help" type="list">帮助</div>
            <%}else if(navType==='workdetail'){%>
                <div class="btn btn_sm_14 bg_23a c_white btn_submit_work" href="/w-4481-690-submit.html">交作业</div>
            <%}%>
        </div>
    </div>